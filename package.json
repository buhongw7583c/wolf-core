      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  },
  "keywords": [
    "botbuilder",
    "Microsoft Bot Framework"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "botbuilder": "^4.0.0-m1.10",
    "botbuilder-node": "^4.0.0-m1.10",
    "botbuilder-services": "^4.0.0-m1.10",
    "bravey": "^0.1.5",
    "jest": "^22.4.2",
    "restify": "^6.3.4",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash.difference": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  }
